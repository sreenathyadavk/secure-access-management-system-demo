# Secure Access Management System - Environment Variables
# Copy to .env and fill in values. Never commit .env.

# Required
DATABASE_URL=postgresql://user:password@localhost:5432/secure_access
JWT_SECRET=your-long-random-secret-at-least-32-chars

# Optional
PORT=3000
NODE_ENV=development
JWT_EXPIRES_IN=15m
BCRYPT_ROUNDS=10
CORS_ORIGIN=*
